                            li
                                a(href="#", title="Analytics22") Promote 2
                            li
                                a(href="#", title="Analytics22") Promote 3
                    li.crm__menu-list
                        a(href="#", title="Analytics22").crm__menu-link
                            img(src='img/ico/help.svg' alt='help')
                            span Help
                        ul.crm__menu-sub
                            li
                                a(href="#", title="Analytics22").active Help 1
